{
  "manifest_version": 3,
  "name": "Reporteur",
  "version": "1.0",
  "description": "Une extension qui raporte les site frauduleux",
  "permissions": [
    "tabs",
    "activeTab"
  ],
  "action": {
    "default_popup": "popup.html",
    "default_icon": {
        "128": "img/icon.png"
    }
  },
  "web_accessible_resources": [
    {
      "resources": [
        "../assets/data/data.json"
      ],
      "matches": [
        "<all_urls>"
      ]
    }
  ]
}